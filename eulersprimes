MAX= int(input("To what number should I count primes? "))
intlist = list(range(2, MAX+1))
i = 0
p = 2
if p != MAX:
    print(intlist)
    for x in intlist:
        n = MAX//p
        a=2
        n1 = a*p
        nlist = []
        while a != n:
            n1 = a*p
            nlist.append(n1)
            a += 1
        print(nlist)
        
print(intlist)
            
        